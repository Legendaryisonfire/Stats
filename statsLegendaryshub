    if type(v) == "table" and rawget(v, "handles") then
        v.freethrow = math,huge;
        v.steal = math.huge;
        v.shotcontest = math.huge;
        v["2pointer"] = 99;
        v["3pointer"] = 99;
        v.passing = math.huge;
        v.layup = math.huge;
        v.strength = math.huge;
        v.dunk = 99;
        v.movingshot = 99;
        v.defense = math.huge;
        v.stamina = math.huge;
    end
end
end)
end)


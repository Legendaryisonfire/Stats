legitstat.Name = "legitstat"
legitstat.Parent = stats
legitstat.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
legitstat.Position = UDim2.new(-0.00101321144, 0, 0.0162929632, 0)
legitstat.Size = UDim2.new(0, 200, 0, 30)
legitstat.Font = Enum.Font.GothamBold
legitstat.Text = "Legit Stat(OP)"
legitstat.TextColor3 = Color3.fromRGB(255, 255, 255)
legitstat.TextScaled = true
legitstat.TextSize = 14.000
legitstat.MouseButton1Down:connect(function()
Gamer:Toggle("Insane Legit Stat",function(value)
for i,v in pairs(getgc(true)) do
    if type(v) == "table" and rawget(v, "handles") then
        v.freethrow = math,huge;
        v.steal = math.huge;
        v.shotcontest = math.huge;
        v["2pointer"] = 99;
        v["3pointer"] = 99;
        v.passing = math.huge;
        v.layup = math.huge;
        v.strength = math.huge;
        v.dunk = 99;
        v.movingshot = 99;
        v.defense = math.huge;
        v.stamina = math.huge;
    end
end
end)
end)


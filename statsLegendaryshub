stats.Name = "stats"
stats.Parent = ScreenGui
stats.Active = true
stats.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
stats.Position = UDim2.new(0.3436988, 0, 0.647852778, 0)
stats.Size = UDim2.new(0, 291, 0, 247)
stats.Visible = true
stats.Active = true
stats.Draggable = true

customstat.Name = "customstat"
